OTELCOL_OPTIONS={{ collector_splunk_otel_collector_command_line_args }}
SPLUNK_CONFIG={{ collector_splunk_otel_collector_config }}
SPLUNK_ACCESS_TOKEN={{ collector_splunk_access_token }}
SPLUNK_REALM={{ collector_splunk_realm }}
SPLUNK_API_URL={{ splunk_api_url | default("https://api." + collector_splunk_realm + ".signalfx.com") }}
SPLUNK_INGEST_URL={{ splunk_ingest_url | default("https://ingest." + collector_splunk_realm + ".signalfx.com") }}
{% if splunk_hec_url is defined and splunk_hec_url %}
SPLUNK_HEC_URL={{ splunk_hec_url }}
{% elif splunk_ingest_url is defined and splunk_ingest_url %}
SPLUNK_HEC_URL={{ splunk_ingest_url + "/v1/log" }}
{% else %}
SPLUNK_HEC_URL={{ "https://ingest." + collector_splunk_realm + ".signalfx.com/v1/log" }}
{% endif %}
SPLUNK_HEC_TOKEN={{ splunk_hec_token }}
SPLUNK_MEMORY_TOTAL_MIB={{ collector_splunk_memory_total_mib }}
{% if collector_splunk_listen_interface is defined and collector_splunk_listen_interface %}
SPLUNK_LISTEN_INTERFACE={{ collector_splunk_listen_interface }}
{% endif %}
{% if collector_gomemlimit is defined and collector_gomemlimit %}
GOMEMLIMIT={{ collector_gomemlimit }}
{% endif %}
SPLUNK_BUNDLE_DIR={{ collector_splunk_bundle_dir }}
SPLUNK_COLLECTD_DIR={{ collector_splunk_collectd_dir }}
{% if collector_splunk_otel_collector_additional_env_vars is not none %}
{% for key,value in collector_splunk_otel_collector_additional_env_vars.items() %}
{{ key }}={{ value }}
{% endfor %}
{% endif %}
